{
  "name": "@bunicorn/client",
  "version": "0.0.18",
  "description": "Bunicorn client whhich has full type support for the Bunicorn server, ultra fast Bun, Deno and Edge server framework",
  "type": "module",
  "private": false,
  "scripts": {
    "build": "tsc && bun run build.js",
    "pretty-lint": "prettier --loglevel silent --write --cache \"**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}\" && eslint . --cache --ext .ts,.tsx --fix",
    "prebuild": "rm -rf dist",
    "update-deps": "ncu -u"
  },
  "source": "src/index.ts",
  "main": "dist/index.js",
  "unpkg": "dist/index.js",
  "jsdelivr": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "keywords": [
    "bunicorn",
    "client",
    "fast",
    "edge",
    "bun",
    "node"
  ],
  "devDependencies": {
    "@bunicorn/server": "workspace:*"
  },
  "homepage": "https://github.com/ragokan/bunicorn",
  "author": "ragokan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ragokan/bunicorn.git",
    "directory": "packages/client"
  },
  "publishConfig": {
    "access": "public"
  }
}
